<% content_for :title, "Edit " + @agent.name -%>

<div class="container">
  <%= render 'agents/nav', agent: @agent, current_workflow: @workflow %>

  <hr>

  <div class="row">
    <div class="col-md-12">
      <div class="page-header">
        <h1>
          Editing your <%= @agent.human_type %>
          <i id="agent-spinner" class="fa fa-spinner fa-pulse spinner"></i>
        </h1>
      </div>
    </div>
  </div>
  <div id="agent-form">
    <%= render 'form' %>
  </div>

</div>
