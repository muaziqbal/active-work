<!-- Nav tabs -->
<ul id="resultTabs" class="nav nav-tabs agent-dry-run-tabs" role="tablist">
  <li role="presentation" class="<%= @results[:messages].empty? ? '' : 'active' %>"><a href="#tabMessages" aria-controls="tabMessages" role="tab" data-toggle="tab">Messages</a></li>
  <li role="presentation" class="<%= @results[:messages].empty? ? 'active' : '' %>"><a href="#tabLog" aria-controls="tabLog" role="tab" data-toggle="tab">Log</a></li>
  <li role="presentation"><a href="#tabMemory" aria-controls="tabMemory" role="tab" data-toggle="tab">Memory</a></li>
</ul>
<!-- Tab panes -->
<div class="tab-content">
  <div role="tabpanel" class="tab-pane <%= @results[:messages].empty? ? '' : 'active' %>" id="tabMessages">
    <pre class="agent-dry-run-messages"><%= Utils.pretty_print(@results[:messages], false) %></pre>
  </div>
  <div role="tabpanel" class="tab-pane <%= @results[:messages].empty? ? 'active' : ''%>" id="tabLog">
    <pre><small class="agent-dry-run-log"><%= @results[:log] %></small></pre>
  </div>
  <div role="tabpanel" class="tab-pane" id="tabMemory">
    <pre class="agent-dry-run-memory"><%= Utils.pretty_print(@results[:memory], false) %></pre>
  </div>
</div>
